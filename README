var LIVR = require('LIVR');

var validator = new LIVR.Validator({
    name: { min_length: 2 },
    email: ['required', { max_length: 2 } ]
});

var clean_data = validator.validate({name:'a', email: "123"});

if (clean_data) {
    console.log('clean_data', clean_data);
} else {
    console.log('errors', validator.getErrors());
}